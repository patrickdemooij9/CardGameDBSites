@using SkytearHorde.Entities.Models.ViewModels
@model RandomizerResultViewModel

<div class="w-full flex md:flex-row flex-col mt-4">

@foreach (var result in Model.Result)
{
    <div class="flex flex-col align-center justify-center gap-2 basis-full">
        <h3 class="text-base font-bold">@result.Key</h3>
        <span>@result.Value</span>
    </div>
}

</div>